<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:OptimizerAvalonia.ViewModels"
             xmlns:models="using:HeatProductionOptimization"
             xmlns:views="using:OptimizerAvalonia.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OptimizerAvalonia.Views.optimizeSettingsView"
             x:DataType="vm:optimizeSettingsViewModel">
  

        
        
    <StackPanel Orientation="Horizontal" Margin="5">
        <TextBlock VerticalAlignment="Center" Margin="0,0,10,0" Text="Cost"/>
        <ToggleSwitch OnContent="{x:Null}" OffContent="{x:Null}" IsChecked="{Binding IsEmissions}" />
        <TextBlock VerticalAlignment="Center" Text="Emissions"/>
    </StackPanel>

        



</UserControl>
