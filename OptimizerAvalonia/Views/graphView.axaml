<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:OptimizerAvalonia.ViewModels"
             xmlns:models="using:HeatProductionOptimization"
             xmlns:views="using:OptimizerAvalonia.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OptimizerAvalonia.Views.graphView"
             x:DataType="vm:graphViewModel"
              xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia">


    <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="50"/>
      <RowDefinition Height="*"/>
    </Grid.RowDefinitions>
    <StackPanel Grid.Row="0" Orientation="Horizontal" Background="Gray" >
        <Button Margin="6" Command="{Binding AddItemCommand}">Add</Button>
        </StackPanel>
<lvc:CartesianChart
        Series="{Binding Series}"
        XAxes="{Binding XAxes}"
        Grid.Row="1">

    </lvc:CartesianChart>
    </Grid>
</UserControl>
